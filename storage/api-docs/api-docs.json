{
    "openapi": "3.0.0",
    "info": {
        "title": "BdShop API",
        "license": {
            "name": "MIT"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://api.laravel-swagger-tutorial.test/",
            "description": "API server"
        },
        {
            "url": "http://localhost:8000/",
            "description": "development-server"
        }
    ],
    "paths": {
        "/users": {
            "get": {
                "tags": [
                    "User"
                ],
                "summary": "List all users",
                "operationId": "index",
                "parameters": [
                    {
                        "name": "include",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "enum": [
                                    "user"
                                ]
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "An paged array of posts"
                    },
                    "default": {
                        "description": "unexpected error"
                    }
                }
            }
        }
    }
}